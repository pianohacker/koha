                            <td>{{ choice.author }}</td>
                            <td>{{ choice.barcode }}</td>
                            <td>
                                <span ng-hide="choice.available">
                                    Available
                                </span>
                                <span ng-show="choice.available">
                                    Not Available
                                </span>
                            </td>
